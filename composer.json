{
    "name": "mrjmpl3/laravel-restful-helper",
    "version": "4.0.0-alpha.8",
    "type": "library",
    "description": "Laravel Helper to Restful projects",
    "keywords": [
        "mrjmpl3",
        "laravel",
        "laravel-restful-helper"
    ],
    "homepage": "https://github.com/MrJmpl3/laravel-restful-helper",
    "license": "MIT",
    "authors": [
        {
            "name": "MrJmpl3",
            "email": "jmpl3.soporte@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/contracts": "^9.0",
        "illuminate/database": "^9.0",
        "illuminate/support": "^9.0",
        "spatie/laravel-json-api-paginate": "^1.11",
        "spatie/laravel-query-builder": "^5.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23",
        "friendsofphp/php-cs-fixer": "^3.7",
        "jetbrains/phpstorm-attributes": "^1.0",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.1",
        "orchestra/testbench": "^7.1",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "spatie/laravel-ray": "^1.29"
    },
    "autoload": {
        "psr-4": {
            "MrJmpl3\\LaravelRestfulHelper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrJmpl3\\LaravelRestfulHelper\\Tests\\": "tests"
        }
    },
    "scripts": {
        "larastan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "php-cs-fixer": [
            "./vendor/bin/php-cs-fixer fix --allow-risky=yes"
        ],
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrJmpl3\\LaravelRestfulHelper\\LaravelRestfulHelperServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
